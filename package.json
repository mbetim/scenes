{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "scenes",
  "title": "Scenes",
  "description": "",
  "icon": "command-icon.png",
  "author": "mbetim",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "create-scene",
      "title": "Create scene",
      "description": "Create a new scene",
      "mode": "view"
    },
    {
      "name": "list-scenes",
